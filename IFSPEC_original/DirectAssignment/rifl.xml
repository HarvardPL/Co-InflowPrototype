<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE riflspec SYSTEM "../rifl-JavaSourceCode-1.0.dtd">
<riflspec>
  <interfacespec>
    <assignable handle="h">
      <category name="highSource">
        <source><parameter   class="DirectAssignment" method="leakyMethod(int)" parameter="1"/></source>
      </category>
    </assignable>
    <assignable handle="l">
      <category name="lowSink">
        <sink><returnvalue class="DirectAssignment" method="leakyMethod(int)"/></sink> 
      </category>
    </assignable>
  </interfacespec>

  <domains><domain name="high" /><domain name="low" /></domains>
  <flowrelation><flow from="low" to="high" /></flowrelation>
  <domainassignment>
    <assign handle="h" domain="high" />
    <assign handle="l" domain="low" />
  </domainassignment>
</riflspec>
