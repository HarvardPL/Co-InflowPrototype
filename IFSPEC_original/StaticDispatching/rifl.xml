<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE riflspec SYSTEM "../rifl-JavaSourceCode-1.0.dtd">
<riflspec>
	<interfacespec>
		<assignable handle="h">
			<category name="hh">
				<source>
					<field class="StaticDispatching" name="h" />
				</source>				
				<sink>
					<field class="StaticDispatching" name="hsink"/>
				</sink>
			</category>
			
		</assignable>

		<assignable handle="l">
			<category name="ll">
				<source>
					<field class="StaticDispatching" name="l" />
				</source>				
				<sink>
					<field class="StaticDispatching" name="lsink"/>
				</sink>
			</category>
		</assignable>
		
	</interfacespec>

	<domains>
		<domain name="high" />
		<domain name="low" />
	</domains>

	<flowrelation>
		<flow from="low" to="high" />
	</flowrelation>

	<domainassignment>
		<assign handle="h" domain="high" />		
		<assign handle="l" domain="low" />
	</domainassignment>

</riflspec>
