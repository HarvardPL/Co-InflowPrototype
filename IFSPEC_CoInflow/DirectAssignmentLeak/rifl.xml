<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE riflspec SYSTEM "../rifl-JavaSourceCode-1.0.dtd">
<riflspec>
  <interfacespec>
    <assignable handle="h">
      <category name="someCat">
        <source><parameter class="Eg1"
	method="f(int,int)" parameter="1"/></source>
      </category>
    </assignable>

    <assignable handle="l">
      <category name="anotherCat">
        <sink><returnvalue class="Eg1"
	method="f(int,int)"/></sink>
      </category>
    </assignable>

  </interfacespec>

  <domains><domain name="high" /><domain name="low" /></domains>

  <flowrelation><flow from="low" to="high" /></flowrelation>

  <domainassignment>
    <assign handle="h" domain="high" />
    <assign handle="l" domain="low" />
  </domainassignment>
</riflspec>

