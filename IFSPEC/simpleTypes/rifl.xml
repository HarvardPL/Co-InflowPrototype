<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE riflspec SYSTEM "../rifl-JavaSourceCode-1.0.dtd">
<riflspec>
    <interfacespec>
        <assignable handle="h">
            <source><field class="simpleTypes" name="secret"/></source>
        </assignable>
        <assignable handle="l">
            <sink><returnvalue class="simpleTypes" method="test()"/></sink>
        </assignable>
    </interfacespec>
    <domains><domain name="high"/><domain name="low"/></domains>
    <flowrelation><flow from="low" to="high"/></flowrelation>
    <domainassignment>
        <assign handle="h" domain="high"/>
        <assign handle="l" domain="low"/>
    </domainassignment>
</riflspec>
